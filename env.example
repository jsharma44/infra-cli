
EMAIL=me@example.com
DOMAIN=localhost
DEPLOYMENT_ENV=local

#SSL Certificate Configuration
SSL_COUNTRY=IN
SSL_STATE=State
SSL_CITY=City
SSL_ORG=YourCompany
SSL_OU=IT
SSL_CN=YourCompany CA
SSL_PREFIX=my-company

#DOCKER
DOCKER_USER=user
USER_PASSWORD=12345
DOCKER_NETWORK=my-network
CLOUDBEAVER_PLATFORM=linux/amd64

#MYSQL
MYSQL_ROOT_PASSWORD=password
MYSQL_PORT=13504

#POSTGRES
POSTGRES_PASSWORD=password
POSTGRES_USER=postgres
POSTGRES_DB=postgres
POSTGRES_PORT=13505

#REDIS
REDIS_PASSWORD=password
REDIS_PORT=6379
REDIS_SSL_PORT=6380
REDIS_USER=default

#FLEET
FLEET_MYSQL_DATABASE=fleet
FLEET_MYSQL_USERNAME=fleet
FLEET_MYSQL_PASSWORD=password
FLEET_SERVER_PRIVATE_KEY="private-key"
FLEET_MDM_WINDOWS_WSTEP_IDENTITY_CERT_BYTES="
-----BEGIN CERTIFICATE-----
your certificate
-----END CERTIFICATE-----
"
FLEET_MDM_WINDOWS_WSTEP_IDENTITY_KEY_BYTES="
-----BEGIN RSA PRIVATE KEY-----
your certificate
-----END RSA PRIVATE KEY-----
"



#TINYBIRD
TB_INFRA_TOKEN=tb_token
TB_INFRA_WORKSPACE=workspace
TB_INFRA_ORGANIZATION=example.com
TB_INFRA_USER=${EMAIL}



#CLOUDFLARE
CLOUDFLARE_API_TOKEN=your_cloudflare_token


# Backup settings
BACKUP_ENABLED=true
BACKUP_RETENTION_DAYS=30
BACKUP_COMPRESSION=true

# S3 Configuration for backups
S3_BACKUP_ENABLED=true
S3_BUCKET_NAME=your-backup-bucket
S3_REGION=ap-south-1
S3_ACCESS_KEY_ID=your_s3_access_key
S3_SECRET_ACCESS_KEY=your_s3_secret_key
S3_ENDPOINT_URL=https://s3.amazonaws.com

# Local backup directory
BACKUP_LOCAL_DIR=./backups
